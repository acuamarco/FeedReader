@model FeedReader.Repository.Model.Article

@{
    ViewBag.Title = Model.Title;
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<h3>@Model.Title</h3>
<div>
    <div>
        @Html.Raw(Model.Body)
    </div>
    <div>
        <img src="@Model.Image" />
    </div>
    <div>
        @Model.PublishedDate
    </div>
</div>